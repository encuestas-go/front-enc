{
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "parcel": "^2.11.0",
    "prettier": "^3.2.5"
  },
  "scripts": {
    "start": "parcel src/html/*.html",
    "build": "parcel build src/html/*.html --dist-dir dist",
    "lint": "eslint src/js/*.js",
    "format": "prettier --write 'src/**/*.{js,css,html}'",
    "test": "jest"
  }
}
